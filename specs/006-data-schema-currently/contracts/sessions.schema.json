{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Session",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string"
      },
      "talk_ids": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "track": {
        "type": "string"
      },
      "time_start": {
        "type": "string",
        "pattern": "^([0-1]?[0-9]|2[0-3]):[0-5][0-9]$"
      },
      "time_end": {
        "type": "string",
        "pattern": "^([0-1]?[0-9]|2[0-3]):[0-5][0-9]$"
      },
      "room": {
        "type": "string"
      },
      "title": {
        "type": "string"
      },
      "level": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "tech_tags": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "description": {
        "type": "string"
      }
    },
    "required": [
      "id",
      "talk_ids",
      "track",
      "time_start",
      "time_end",
      "room",
      "title"
    ]
  }
}
